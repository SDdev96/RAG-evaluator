# Esempio di configurazione delle variabili d'ambiente
# Copia questo file in .env e inserisci le tue API keys

# OpenAI API Key (richiesta per embeddings e semantic chunking)
OPENAI_API_KEY=your_openai_api_key_here

# Google API Key (richiesta per Gemini generation)
GOOGLE_API_KEY=your_google_gemini_api_key_here

# Configurazioni opzionali
LOG_LEVEL=INFO
CACHE_ENABLED=true
PARALLEL_PROCESSING=true

# Configurazioni avanzate (opzionali)
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
VECTOR_WEIGHT=0.7
BM25_WEIGHT=0.3
TOP_K_RESULTS=5
